    }

/* the styles for the global XHTML elements */
    a,a:link{
	    color:#2020c0;
	    text-decoration:none;
    }

    a:hover{
	    color:#dd0000;
    }

    div.error{
	    font-size:1.0em;
	    color:#ff0000;
    }

    div.rendered{
	    font-size:0.8em;
    }

    div.widget_std {
        width: 500px;
        float: left;
    }
     
    div.widget_reset {
        clear: both;
    }
     
    fieldset {
        margin-top: 1em;
        margin-bottom: 1em;
    }

    h1 {
	    font-size:1.5em;
        margin-top:0.5em;
        margin-bottom:0.5em;
	    color:#090940;
    }

    h2{
	    font-size:1.3em;
        margin-top:0.5em;
        margin-bottom:0.5em;
	    color:#090940;
    }

    h3{
	    font-size:1.1em;
	    color:#090940;
    }

    h4{
	    margin-top:0px;
	    margin-bottom:0px;
	    font-size:0.9em;
	    color:#090940;
    }

    h1.print {
	    font-size:1.6em;
	    color:#000000;
    }

    h2.print{
	    font-size:1.4em;
	    color:#000000;
    }

    h3.print{
	    font-size:1.1em;
	    color:#000000;
    }

    h4.print{
	    margin-top:0px;
	    margin-bottom:0px;
	    font-size:1.0em;
	    color:#000000;
    }

    p,li,td{
	    font-size:1.0em;
    }

    legend {
        color: black;
        font-weight: bold;
        font-size: 100%;
    }

    option.monosp {
	    font-family: Courier, Helvetica, sans-serif;
	    color: #000000;
	    background-color: #FFFFFF;
	    font-size:1.2em;
	    font-weight: normal
    }

    span.mandatory_field{
	    color: red;    
    }

    span.error_field{
	    color: red;    
    }

    table.line {
	    font-family: Arial, Helvetica, sans-serif;
	    color: #000000;
	    background-color: #FFFFFF;
	    font-weight: normal;
        border-collapse: collapse
    }

    table.tablebg_blue td{
        vertical-align: top;
	    background-color: #FAFAFF;
    }

    table.valigntop td{
        vertical-align: top;
    }

    table.print {
	    font-family: Arial, Helvetica, sans-serif;
	    color: #000000;
	    background-color: #FFFFFF;
	    font-weight: normal;
        border-collapse: collapse;
    }

    table.print td{
        padding-left: 5px;
        padding-right: 10px;
        border-collapse: collapse;
        vertical-align: top;
    }

    table.figure {
	    font-family: Arial, Helvetica, sans-serif;
	    color: #000000;
	    background-color: #FFFFFF;
        border-collapse: collapse;
    }

    table.figure td{
	    font-weight: 0.5em;
        padding-left: 0px;
        padding-right: 0px;
    }

    td.banner {
	    background-color: #554513;
	    font-family: Verdana, Arial, sans-serif;
	    font-size:0.6em;
	    color: #ffff44;
        padding-right:2px;
    }

    td.banner_button_enabled {
        text-align: center;
	    color: black;
    }

    td.banner_button_disabled {
        text-align: center;
	    color: gray;
    }

    td.print_label {
	    font-size:1.2em;
    }

    td.print_labelS {
	    font-size:0.9em;
    }

    td.print_data {
	    font-size:1.2em;
        padding-bottom:4px;
    }

    td.print_dataL {
	    font-size:2.0em;
        padding-bottom:20px;
    }

    td.print_border_TopRightBottonLeft {
        border-width: thin;
        border-style: solid solid solid solid;
    }

    td.print_border_trBottoml {
        border-width: thin;
        border-style: none none solid none;
    }

    td.print_border_trbl {
        border-style: solid solid solid none;
    }

    td.print_border_trbl {
        border-style: solid solid solid none;
    }


     #nav{
        float:left;
        width:135px;
        height:auto;
        border:2px solid #31363E;
     }
     

    #nav ul{
	    margin: 0;
	    padding: 0;
	    list-style: none;	
    }

    #nav ul li.cat{
	    background-color:#31363e;
	    font-size:1em;
	    padding-left:5px;
    }

    #nav ul li.subcat{
	    padding-left:15px;
	    font-size:.9em;
    }

    #nav li.cat a {
	    text-decoration: none;
	    color: #e4deae;
	    border: none;
    }

    #nav li.subcat a {
	    text-decoration: none;
	    color: #000;
	    border: none;
    }


	
	
    #nav li.subcat a:hover {
	    text-decoration:none;
	    color:#14b8b8;
    }
    #nav li.cat a:hover {
	    text-decoration:none;
	    color:#14b8b8;
    }
     

     
